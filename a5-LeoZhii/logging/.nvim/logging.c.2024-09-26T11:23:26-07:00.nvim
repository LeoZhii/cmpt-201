#include "log.h"
#include <stdio.h>

int main() {
  int size = 5;
  int numbers[size];
  int sum = 0;

  for (int i = 0; i <= size; ++i) {
    numbers[i] = i;
    sum += numbers[i];
    log_trace("Iteration %d, the running sum is %d", i, sum);
  }

  log_trace("The final sum is: %d", sum);
  printf("The sum is: %d\n", sum);

  return 0;
}
